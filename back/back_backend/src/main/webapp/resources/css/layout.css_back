input[type="checkbox"] {  /* 실제 체크박스는 화면에서 숨김 */
  width: 1px;
  height: 1px;
  padding: 0;
  margin: 0;
  overflow: hidden;
  clip:rect(0,0,0,0);
  border: 0;
  position: absolute;
  left:0;
}
input[type="text"].gray{
  background-color: #eee;
}

input[type="checkbox"] + label {
  display: inline-block;
  position: relative;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  margin:0;
  font-weight:normal;
  position: relative;
  left:0;
  color:#999;
  font-size: 14px;
  margin-right: 5px;
}
input[type="checkbox"] + label:before {  /* 가짜 체크박스 */
  content: '';
  display:inline-block;
  width: 24px;  /* 체크박스의 너비를 지정 */
  height: 24px;  /* 체크박스의 높이를 지정 */
  line-height: 26px; /* 세로정렬을 위해 높이값과 일치 */
  text-align: center;
  vertical-align: middle;
  border-radius : 5px;
  border:1px solid  rgba(0,0,0,.1);
  margin-right: 5px;
  text-align: center;
}
input[type="checkbox"]:checked + label:before {  /* 체크박스를 체크했을때 */
  content: '●';
  color:#fff;
  background-color: rgba(57,65,87,1);
  border-color: #fff;
}



input[type="radio"] {  /* 실제 체크박스는 화면에서 숨김 */
  width: 1px;
  height: 1px;
  padding: 0;
  margin: 0;
  overflow: hidden;
  clip:rect(0,0,0,0);
  border: 0;
  position: absolute;
  left:0;
}

input[type="radio"] + label {
  display: inline-block;
  position: relative;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  margin:0;
  font-weight:normal;
  left:0;
  color:#999;
  font-size: 12px;
  margin-right: 5px;
}
input[type="radio"] + label:before {  /* 가짜 체크박스 */
  content: '';
  display:inline-block;
  width: 16px;  /* 체크박스의 너비를 지정 */
  height: 16px;  /* 체크박스의 높이를 지정 */
  line-height: 18px; /* 세로정렬을 위해 높이값과 일치 */
  text-align: center;
  vertical-align: middle;
  border-radius : 20px;
  border:1px solid  rgba(0,0,0,.1);
  margin-right: 5px;
}
input[type="radio"]:checked + label:before {  /* 체크박스를 체크했을때 */
  content: '●';
  color:#fff;
  background-color: rgba(57,65,87,1);
  border-color: rgba(57,65,87,1);
}
