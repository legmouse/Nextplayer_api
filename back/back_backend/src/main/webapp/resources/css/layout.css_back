.tab {
  clear:both;
  height:34px;
  margin-bottom: 5px;
  width:100%;
  box-sizing: border-box;
  height:50px;
  width:100%;
  margin-top: 32px;
  display: block;
}
.tab a{
  color:#111;
  border-radius: 0;
  float:left;
  text-align: center;
  box-sizing: border-box;
  padding: 10px 30px;
  color:#bbb;
  cursor: pointer;
  font-size:16px;
}
.tab a:hover{
  background-color: rgba(0,0,0,.1);
  border-radius: 5px 5px 0 0;
  border-top: rgba(0,0,0,.1);
  border-left: rgba(0,0,0,.1);
  border-right: rgba(0,0,0,.1);
}
.tab a.active{
  color:#111;
  border:1px solid rgba(0,0,0,.1);
  border-bottom:1px solid #fff;
  border-radius: 5px 5px 0 0;
  margin-top: 1px;
  font-weight:bold;
  background-color: #fff;
}
.tab.tab-small{
  border-top:1px solid rgba(0,0,0,.1);
  height:40px;
}
.tab.tab-small a{
  height:40px;
  line-height: 40px;
  position: relative;
  font-size:14px;
  color:#999;
  border-bottom: 0;
}
.tab.tab-small a span{
  display: block;
  position: absolute;
  top:-1px;
  left: 10%;
  right:10%;
  height:3px;
  /*background-color: rgba(0,0,0,.1);*/
}
.tab.tab-small a.active {
  color: #0C3EDF;
  font-weight: bold;
}
.tab.tab-small a.active span{
  display: block;
  position: absolute;
  top:-1px;
  left: 10%;
  right:10%;
  height:3px;
  background-color: #0C3EDF;
}


.tab.tab-small.down{
  border-bottom:1px solid rgba(0,0,0,.1);
  border-top:0;
  height:40px;
}
.tab.tab-small.down a span{
  display: block;
  position: absolute;
  bottom:-1px;
  left: 10%;
  right:10%;
  height:3px;
  /*background-color: rgba(0,0,0,.1);*/
}
.tab.tab-small.down a.active span{
  display: block;
  position: absolute;
  bottom:-1px;
